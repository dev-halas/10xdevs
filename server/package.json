{
    "name": "server",
    "version": "1.0.0",
    "type": "commonjs",
    "scripts": {
        "server": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc -p .",
        "start": "node dist/server.js",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:unit": "vitest run --exclude src/tests/e2e/**",
        "test:e2e": "vitest run src/tests/e2e",
        "test:e2e:watch": "vitest src/tests/e2e",
        "prisma": "prisma",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev --name init",
        "prisma:studio": "prisma studio"
    },
    "dependencies": {
        "@fastify/cookie": "^11.0.2",
        "@fastify/cors": "^11.2.0",
        "@fastify/helmet": "^13.0.2",
        "@prisma/client": "^6.19.2",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.3",
        "fastify": "^5.7.1",
        "ioredis": "^5.9.2",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@types/jsonwebtoken": "^9.0.7",
        "@types/bcryptjs": "^2.4.6",
        "@types/node": "^24.10.9",
        "@vitest/coverage-v8": "^2.1.9",
        "dotenv": "^17.2.3",
        "prisma": "^6.19.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3",
        "vitest": "^2.1.9"
    }
}